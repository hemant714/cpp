#include<iostream>
using namespace std;
main()
{
int i,j,f,n;
cin>>n;
f=(n)/2;
 for(i=1;i<=n;i++)
 {
 for(j=1;j<=n;j++)
   {
       if(i<=f & ( j>1 & j<=f))
        cout<<" ";
      else  if((i>(f+1) & i<n) & (j<=f) )
            cout<<" ";
        else if((i>1 & i<=f) & j>(f+1))
            cout<<" ";
        else if(i>(f+1) & (j>(f+1) & j<n))
                cout<<" ";

       else
    cout<<"*";
   }
   cout<<endl;
 }

}
